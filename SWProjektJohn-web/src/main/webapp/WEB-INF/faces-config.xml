<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
        http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
              version="2.2">
    <!--lifecycle>
        <phase-listener>de.oth.joa44741.swprojektjohn.web.jsf.MyPhaseListener</phase-listener>
    </lifecycle-->
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>festivaldetails</from-outcome>
            <to-view-id>/festivaldetails.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>bandDetails</from-outcome>
            <to-view-id>/bandDetails.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>editMainFestivalData</from-outcome>
            <to-view-id>/editMainFestivalData.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>insertMainFestivalData</from-outcome>
            <to-view-id>/insertMainFestivalData.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>insertCampingAndTickets</from-outcome>
            <to-view-id>/insertCampingAndTickets.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>insertLineup</from-outcome>
            <to-view-id>/insertLineup.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>